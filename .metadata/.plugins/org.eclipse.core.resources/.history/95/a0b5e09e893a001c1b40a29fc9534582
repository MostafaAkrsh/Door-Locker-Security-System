#include "buzzer.h"
#include "gpio.h"

/* port and pin to only used in the same file */
static uint8 port;
static uint8 pin;

void Buzzer_init(uint8 Port , uint8 Pin)
{
	/* Make the wanted pin Output Pin */
	GPIO_setupPinDirection(Port, Pin, PIN_OUTPUT);
}
