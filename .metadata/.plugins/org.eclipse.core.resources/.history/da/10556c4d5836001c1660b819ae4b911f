#include "timer.h"
#include "avr/io.h"
#include <avr/interrupt.h>

ISR(TIMER0_OVF_vect)
{
	PORTC = ~PORTC;
}

int main()
{
	TIMER_Config config;

	SREG |= (1<<7);
	DDRC = 0xFF;
	PORTC = 0xFF;
	config.timer_id = TIMER0_ID;
	config.timer_mode = OVERFLOW_MODE;
	config.timer_clk =  NO_PRESCALER;
	config.oc_mode = DISCONNECTED;
	config.interrupt = ON;
	config.inital_value = 0;
	config.compare_value = 155;

	Timer_Init(&config);

	while(1)
	{

	}
}
